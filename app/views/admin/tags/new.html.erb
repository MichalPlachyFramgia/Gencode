<%= form_for [:admin, @tag] do |f| %>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :enclose %><br>
    <%= f.check_box :enclose %>
  </div>
  <div class="field">
    <%= f.label :type_id %><br>
     <%= f.select :type_id,
      options_from_collection_for_select(@types, :id, :name) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>